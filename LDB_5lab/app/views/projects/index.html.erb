Projects you manage: <br><br>
<% @projects.each do |proj| %>
  <%= proj.name %> <%= proj.status %> <%= proj.budget %> 
  <%= link_to 'Edit', url_for(:controller => :projects, :method => :edit, :id => proj.id) %>
  <%= link_to 'Delete', url_for(:controller => :projects, :method => :destroy, :id => proj.id) %>
  <%= link_to 'Add member', url_for(:controller => :projects, :method => :addmem, :id => proj.id) %><br>

  Members: <br>
  <% proj.members_getter.each do |membr| %>
    <%= membr unless membr.eql?(nil)%>
    <%= link_to 'Remove', url_for(:controller => :projmems, :id => proj.id, :member => membr) %>
    <br>
  <% end %>

  <br>
<% end %>
<br>
<%= link_to 'Create project', url_for(:controller => :projects, :method => :create) %>

