<h1> Login </h1>

<%= form_for :user, :url => { :action => 'parse_login' } do |form| %>
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>

  <p>
    <%= form.label :password %><br>
    <%= form.password_field :pass %>
  </p>

  <p>
    <%= form.submit 'Login' %>
  </p>
<% end %>
